@{
    ViewBag.CurrentMenu = "EquipmentModel";
}


<!--新增型号 开始-->
<div class="modal fade bg-success"  id="EditModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="EditTitle">新增</h4>
            </div>
            <div class="modal-body">
                <form class="form-horizontal">
                    <div class="box-body">
                        <text id="inputId" hidden="hidden"></text>
                        <div class="form-group">
                            <label for="inputModel" class="col-sm-2 control-label">型号</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="inputModel" placeholder="型号" data-toggle="tooltip" data-placement="top" onblur="initTooltip()">
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label for="inputThumbnail" class="col-sm-2 control-label">图片</label>
                            <div class="col-sm-10">
                                <input type="file" accept="image/jpeg" class="form-control" id="inputThumbnail" data-toggle="tooltip" data-placement="top">
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="inputManufacturer" class="col-sm-2 control-label">品牌</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="inputManufacturer" placeholder="生产厂家">
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="inputScreenSize" class="col-sm-2 control-label"> 屏尺寸</label>
                            <div class="col-sm-10">
                                <input type="number" max="500" min="1" class="form-control" id="inputScreenSize" placeholder="0" data-toggle="tooltip" data-placement="top">
                            </div>

                        </div>
                      
                        <div class="row form-group">
                            <label for="inputResolutionRatioLength" class="col-sm-2 control-label"> 分辨率</label>
                            <div class="col-sm-4">
                                <input type="number" max="10000" min="1" class="form-control" id="inputResolutionRatioLength" placeholder="0">
                            </div>

                            <label for="inputResolutionRatioWidth" class="col-sm-1 control-label"> *&nbsp;&nbsp;</label>   
                            <div class="col-sm-5">
                                <input type="number" max="10000" min="1" class="form-control" id="inputResolutionRatioWidth" placeholder="0">
                            </div>
                        </div>
         
                        <div class="row form-group">
                            <label for="inputScreenWidth" class="col-sm-2 control-label"> 屏宽cm</label>
                            <div class="col-sm-4">
                                <input type="number" max="100000" min="1" class="form-control" id="inputScreenWidth" placeholder="0">            
                            </div>

                            <label for="inputScreenHeight" class="col-sm-2 control-label"> 屏高cm</label>       
                            <div class="col-sm-4">
                                <input type="number" max="100000" min="1" class="form-control" id="inputScreenHeight" placeholder="0">           
                            </div>
                        </div>


                        <div class="form-group">
                            <label for="inpuScreenMaterial" class="col-sm-2 control-label"> 材质</label>
                            <div class="col-sm-10">
                                <select class="form-control select2" style="width: 100%;" id="inpuScreenMaterial" data-toggle="tooltip" data-placement="top"></select>
                            </div>
                        </div>

               
                        <div class="form-group">
                            <label for="inputvoiced" class="col-sm-2 control-label"> 有声音</label>
                            <div class="col-sm-10">
                                <input type="checkbox" id="inputvoiced">
                            </div>
                        </div>


                        <div class="form-group">
                            <label for="inputSideScreensNumber" class="col-sm-2 control-label"> 副屏数</label>
                            <div class="col-sm-10">
                                <input type="number" max="100" min="0" class="form-control" id="inputSideScreensNumber" placeholder="0">
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="inputApplyTo" class="col-sm-2 control-label"> 适用于</label>
                            <div class="col-sm-10">
                                <select class="form-control select2" multiple="multiple" data-live-search="true" style="width: 100%;" id="inputApplyTo"> </select>
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="inputDescribe" class="col-sm-2 control-label"> 简介</label>
                            <div class="col-sm-10">
                                <textarea class="form-control" rows="3" placeholder="简介 ..." id="inputDescribe"></textarea>
                            </div>
                        </div>

                    </div>

      
                    <!-- /.box-body -->
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" id="btnNewCancel" data-dismiss="modal">取消</button>
                        <button type="button" class="btn btn-primary" id="btnNewSave">保存</button>
                    </div>
                    <!-- /.box-footer -->
                </form>
            </div>

        </div>
    </div>
</div>
