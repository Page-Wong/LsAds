<div class="modal  modal-success fade" id="CompleteModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="ConfirmedModalitle">完成</h4>
            </div>
            <div class="modal-body ">
                <form class="form-horizontal">
                    <div class="box-body">
                        <text id="inputIdS" hidden="hidden"></text>


                        <div class="form-group">
                            <label>处理人员:</label>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-user"></i>
                                </div>
                                <input type="text" class="form-control" id="inputProcessingPersonS" disabled="disabled">
                            </div>
                        </div>

                        <div class="form-group">
                            <label>联系电话:</label>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-phone"></i>
                                </div>
                                <input type="text" class="form-control" id="inputProcessingPersonPhoneS" disabled="disabled">
                            </div>
                        </div>

       

                        <div class="form-group">
                            <label>处理方法:</label>
                            <div> &nbsp;&nbsp;&nbsp;
                                <label>
                                   <input type="radio" name="inputProcessingMethod" class="flat-red" id="repair" value="1" checked>
                                    维修&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                </label>
                                <label>
                                    <input type="radio" name="inputProcessingMethod" class="flat-red" id="replace" value="2">
                                   更换&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                </label>
                                <label>
                                    <input type="radio" name="inputProcessingMethod" class="flat-red" id="other" value="3">
                                   其他
                                </label>
                            </div>
                        </div>


                        <div class="form-group">
                            <label>处理描述:</label>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-edit"></i>
                                </div>
                                <textarea class="form-control"  data-toggle="tooltip" data-placement="top" rows="4" placeholder="Enter ..."  id="inputProcessingResults"></textarea>
                            </div>
                        </div>

                    </div>

                    <!-- /.box-body -->
                    <div class="modal-footer">
                        <div class="btn-group pull-right">
                            <button type="button" class="btn btn-primary fa fa-reply" id="CompleteCancel" data-dismiss="modal">取消</button>
                            <button type="button" class="btn btn-primary fa fa-save" id="CompleteSave">保存</button>
                        </div>
                    </div>
                    <!-- /.box-footer -->
                </form>
            </div>

        </div>
    </div>
</div>