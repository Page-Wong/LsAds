@model List<LsAdmin.Application.MaterialApp.Dtos.MaterialDto>

<div class="table-scrollable">
    <input hidden value="@ViewBag.RowCount" name="rowCount" />
    <input hidden value="@ViewBag.PageCount" name="pageCount" />
    <table id="mediaTable" class="table table-striped table-bordered table-hover dataTable no-footer" role="grid" aria-describedby="sample_2_info">
        <thead>
            <tr role="row">
                <th tabindex="0" aria-label="" style="width: 30px" hidden>
                    #
                </th>
                <th tabindex="0" aria-label="">
                    名称
                </th>
                <th tabindex="0" aria-label="" style="width: 100px">
                    持续时间(秒)
                </th>
                <th tabindex="0" aria-label="">
                    备注
                </th>
                <th tabindex="0" style="width: 50px;" aria-label="">
                    操作
                </th>
            </tr>
        </thead>
        <tbody id="materialTableBody">
            @foreach (var item in Model) {
                @Html.Partial("_MaterialTr", item)
            }
        </tbody>
    </table>
</div>